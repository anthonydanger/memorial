<% provide(:title, @tribute.name) %>
<div class="box">
	<aside class="span4">
		<section>
			<h1>
				<%= image_tag @tribute.image.url(:thumb) %>
				<%= @tribute.name %>
			</h1>
			<%= @tribute.name %> was born <%= @tribute.dob %> and passed away one <%= @tribute.dod %> 
			<%= @tribute.obituary %>
		</section>
		Created with love by <%= @user.name %>.
	</aside>
</div>